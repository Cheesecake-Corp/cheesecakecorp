{
  "name": "cheesecakecorp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only docs:build && shx mkdir -p dist/docs && shx cp -r src/markdown/.vitepress/dist/* dist/docs/",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "docs:dev": "vitepress dev src/markdown",
    "docs:build": "vitepress build src/markdown",
    "docs:preview": "vitepress preview src/markdown"
  },
  "dependencies": {
    "JSON": "^1.0.0",
    "react-markdown": "^10.1.0",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/tsconfig": "^0.8.1",
    "markdown-it-mathjax3": "^4.3.2",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "shx": "^0.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitepress": "^2.0.0-alpha.16",
    "vue-tsc": "^3.2.4"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
